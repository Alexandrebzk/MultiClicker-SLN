{
    "version": "0.2.0",
    "configurations": [
        
      {
        "name": ".NET Framework Launch (console)",
        "type": "clr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/Multi-Clicker/bin/Debug/MultiClicker.exe",
        "args": [],
        "cwd": "${workspaceFolder}",
        "stopAtEntry": false,
        "serverReadyAction": {
          "action": "openExternally",
          "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
        },
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        },
        "console": "internalConsole"
      }
    ]
  }